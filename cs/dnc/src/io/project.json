                "../../../src/io/unsafe/*.cs",
                "../../../src/io/properties/*.cs"
            ]
        },
        "debugType": "portable",
        "keyFile": "../../../build/internal/bond.snk",
        "publicSign": true
    },
    "dependencies": {
        "attributes": "1.0.0-*",
        "core": "1.0.0-*",
        "reflection": "1.0.0-*"
    },
    "frameworks": {
