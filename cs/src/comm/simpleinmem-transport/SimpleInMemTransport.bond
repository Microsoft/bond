namespace bond.comm.simpleinmem;

enum SimpleInMemMessageType
{
    Request = 1;
    Response = 2;
    Event = 3;
}

struct SimpleInMemHeaders
{
    0: uint64 conversation_id;
    1: required SimpleInMemMessageType message_type = Request;
    2: string method_name;
}
